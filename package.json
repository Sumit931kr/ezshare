{
  "name": "ezshare",
  "version": "1.11.0",
  "description": "Easily share files over LAN",
  "main": "index.js",
  "repository": "https://github.com/mifi/ezshare",
  "author": "Mikael Finstad <finstaden@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "bin": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "exit 0",
    "build": "cd ezshare-frontend && npm run build",
    "prepack": "npm run build",
    "start": "concurrently -k \"cd ezshare-frontend && yarn start\" \"node --watch . --dev-mode\""
  },
  "files": [
    "/ezshare-frontend/dist/**",
    "app.js"
  ],
  "pkg": {
    "assets": [
      "ezshare-frontend/dist/**"
    ]
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "body-parser": "^1.19.0",
    "clipboardy": "^2.3.0",
    "content-disposition": "^0.5.3",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "filenamify": "^4.2.0",
    "formidable": "^3.5.2",
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^1.0.3",
    "lodash": "^4.17.15",
    "meow": "^13.2.0",
    "morgan": "^1.9.1",
    "p-map": "^4.0.0",
    "qrcode-terminal": "^0.12.0",
    "range-parser": "^1.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^5.1.0",
    "eslint": "^8.2.0",
    "eslint-config-mifi": "^0.0.6",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "typescript": "~5.3.0"
  },
  "packageManager": "yarn@4.5.1"
}
